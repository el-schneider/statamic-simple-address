{
  "name": "el-schneider/statamic-simple-address",
  "description": "A simple automcomplete dropdown for Statamic",
  "license": "MIT",
  "type": "statamic-addon",
  "autoload": {
    "psr-4": {
      "ElSchneider\\StatamicSimpleAddress\\": "src"
    }
  },
  "require": {
    "php": "^8.0",
    "statamic/cms": "^3.0|^4.0"
  },
  "extra": {
    "download-dist": {
      "url": "https://github.com/el-schneider/statamic-simple-address/releases/download/{$version}/dist.tar.gz",
      "path": "resources/dist"
    },
    "statamic": {
      "name": "Statamic Simple Address",
      "description": "A simple automcomplete dropdown for Statamic"
    },
    "laravel": {
      "providers": [
        "ElSchneider\\StatamicSimpleAddress\\ServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true
    }
  }
}
