{
  "name": "el-schneider/statamic-simple-address",
  "description": "A simple address automcomplete for Statamic",
  "license": "MIT",
  "type": "statamic-addon",
  "autoload": {
    "psr-4": {
      "ElSchneider\\StatamicSimpleAddress\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "require": {
    "php": "^8.3",
    "statamic/cms": "^6.0",
    "geocoder-php/nominatim-provider": "^5.8",
    "geocoder-php/cache-provider": "^4.0",
    "guzzlehttp/guzzle": "^7.10"
  },
  "extra": {
    "statamic": {
      "name": "Statamic Simple Address",
      "description": "A simple address automcomplete for Statamic"
    },
    "laravel": {
      "providers": [
        "ElSchneider\\StatamicSimpleAddress\\ServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true,
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "require-dev": {
    "laravel/pint": "^1.22",
    "pestphp/pest": "*",
    "orchestra/testbench": "^10.0"
  }
}
